{% if template contains 'collection'%}
<!-- collection link -->
<link rel="preload" href="{{ 'collection-template.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
<noscript><link rel="stylesheet" href="{{ 'collection-template.css' | asset_url }}"></noscript>
     {% for product in collection.products limit: 2 %}
        {% if product.images.size > 0 %}
          <link rel="preload" href="{{ product.images.first | img_url: '300x300' }}" loading="eager" decoding="async" as="image" fetchpriority="high" type="image/webp" />
        {% endif %}
      {% endfor %} 
{% endif %}
