<script>
  const stickyElement = document.getElementById('stickyViewCart-outer');
  let isScrolling;

  window.addEventListener('scroll', function() {
    // Add the class while scrolling
    stickyElement.classList.add('scrolled');

    // Clear our timeout throughout the scroll
    window.clearTimeout(isScrolling);

    // Set a timeout to run after scrolling ends
    isScrolling = setTimeout(function() {
      // Remove the class when scrolling stops
      stickyElement.classList.remove('scrolled');
    }, 100); // 5 seconds
