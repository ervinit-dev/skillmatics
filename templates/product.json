    "custom_liquid_6RTi9y": {
      "type": "custom-liquid",
      "settings": {
        "custom_code": "<div class=\"container\" id=\"amazon-reviews\">\n  <div class=\"tt-block-title\" id=\"reviews-title\">\n    <p class=\"tt-title\">Customer Reviews</p>\n  </div>\n\n{% if product.metafields.pdp.product_rating != blank %}\n        {% assign rating = product.metafields.pdp.product_rating | times: 1 %}\n        {% assign fullStars = rating | floor %}\n        {% assign decimalPart = rating | modulo: 1 %}\n        {% assign nextStar = fullStars | plus: 1 %}\n\n<header class=\"custom-reviews-head\">\n    <div>\n        <div class=\"reputon-info\">\n            <div class=\"reputon-content\">\n                <div class=\"reputon-name\"></div>\n                <div class=\"custom-rating-stars\">\n                    <div class=\"custom-count-number\">{{ rating }}</div>\n                    <div class=\"reputon-count-text\">\n                      {% for i in (1..5) %}\n              {% assign isFullStar = false %}\n              {% assign isHalfStar = false %}\n\n              {% if i <= fullStars %}\n                {% assign isFullStar = true %}\n              {% elsif i == nextStar and decimalPart >= 0.8 %}\n                {% assign isFullStar = true %}\n              {% elsif i == nextStar and decimalPart >= 0.3 and decimalPart <= 0.7 %}\n                {% assign isHalfStar = true %}\n              {% endif %}\n\n              <svg viewBox=\"0 0 1792 1792\" width=\"17\" height=\"17\" class=\"reputon-icon-star\">\n                <!-- Star Outline -->\n                <path d=\"M1728 647q0 22-26 48l-363 354 86 500q1 7 1 20 0 21-10.5 35.5t-30.5 14.5q-19 0-40-12l-449-236-449 236q-22 12-40 12-21 0-31.5-14.5t-10.5-35.5q0-6 2-20l86-500-364-354q-25-27-25-48 0-37 56-46l502-73 225-455q19-41 49-41t49 41l225 455 502 73q56 9 56 46z\"\n                  stroke=\"#FFCB33\" stroke-width=\"100\" fill=\"none\"/>\n\n                {% if isFullStar %}\n                  <!-- Full Star Fill -->\n                  <path d=\"M1728 647q0 22-26 48l-363 354 86 500q1 